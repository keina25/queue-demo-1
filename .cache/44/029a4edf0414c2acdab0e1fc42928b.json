r\naddChild(node2,203);\r\naddChild(node2,204);\r\nconsole.log(tree);\r\n\r\nconst fn = node =>{\r\n    console.log(node.data)\r\n}\r\n//travel(tree,fn);\r\n//遍历数组,从原本的travel(tree,node =>(console.log(node.data));\r\n//改成了上面const fn = node=>{console.log}\r\n//这样更适合打断点，debugger\r\n\r\n//console.log(find(tree,node2));\r\n//在那棵树里，找到那个2，验证是否能找到20\r\n\r\nremoveNode(tree,node5);\r\nconsole.log(tree);"},"lineCount":null}},"error":null,"hash":"c1deb2d83ae8dbae0b1a7f85cf12177a","cacheData":{"env":{}}}